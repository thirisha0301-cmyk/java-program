 public class Main { 
    static class Student {
        private String name;
        private int marks;  
        private String studentId; 
        public Student(String name, int marks, String studentId) {
            this.name = name;
            this.marks = marks;  
            this.studentId = studentId;
        } 
        public String getName() {
            return name;
        }

        public int getMarks() {
            return marks;  
        }

        public String getStudentId() {
            return studentId;
        } 
        public void displayDetails() {
            System.out.println("Student Details:");
            System.out.println("Name: " + getName());
            System.out.println("Marks: " + getMarks()); // Display marks
            System.out.println("Student ID: " + getStudentId());
            System.out.println();  
        }
    }

    public static void main(String[] args) {
       
        Student student1 = new Student("thirisha", 85, "117");
        Student student2 = new Student("sivatmika", 90, "107"); 
        student1.displayDetails();
        student2.displayDetails();
    }
}
